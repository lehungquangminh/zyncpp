  "publisher": "lehungquangminh",
  "version": "0.1.0",
  "description": "Extension Pack for zynC++",
  "engines": { "vscode": "^1.93.0" },
  "categories": ["Extension Packs"],
  "extensionPack": [
    "zynther.zyn-cpp-core",
